{
  "name": "walletconnect-backend-sdk",
  "version": "1.0.0",
  "description": "A comprehensive WalletConnect SDK for backend implementations with session persistence and multiple database support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "ts-node tests/test-runner.ts",
    "test:core": "ts-node tests/core-functionality.test.ts",
    "test:solana": "ts-node tests/solana-support.test.ts",
    "test:batch": "ts-node tests/batch-transactions.test.ts",
    "test:quick": "ts-node tests/test-runner.ts --quick",
    "test:verbose": "ts-node tests/test-runner.ts --verbose",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "example:quick-start": "ts-node examples/quick-start.ts",
    "example:express-server": "ts-node examples/express-api-server.ts",
    "example:contract-interactions": "ts-node examples/contract-interactions.ts",
    "example:dex-trading-bot": "ts-node examples/dex-trading-bot.ts",
    "example:telegram-bot": "ts-node examples/telegram-bot.ts",
    "telegram-bot": "ts-node examples/telegram-bot.ts",
    "telegram-bot-test": "ts-node examples/telegram-bot-test.ts",
    "example:deep-links": "ts-node examples/wallet-deep-links-timeouts.ts"
  },
  "keywords": [
    "walletconnect",
    "backend",
    "sdk",
    "web3",
    "blockchain",
    "wallet",
    "session",
    "persistence",
    "database"
  ],
  "author": "WalletConnect Backend SDK Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@walletconnect/core": "^2.11.2",
    "@walletconnect/sign-client": "^2.11.2",
    "@walletconnect/types": "^2.11.2",
    "@walletconnect/utils": "^2.11.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "mysql2": "^3.6.5",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "redis": "^4.6.10",
    "sqlite": "^5.0.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "viem": "^2.7.9",
    "ws": "^8.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/node-telegram-bot-api": "^0.64.9",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "dotenv": "^17.2.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "node-telegram-bot-api": "^0.66.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/walletconnect-backend-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/walletconnect-backend-sdk/issues"
  },
  "homepage": "https://github.com/your-org/walletconnect-backend-sdk#readme"
}
